{
  "name": "tiko-mono",
  "version": "1.0.0",
  "description": "Tiko - Accessibility-focused communication apps for children",
  "private": true,
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "packageManager": "pnpm@8.15.0",
  "scripts": {
    "build": "nx run-many --target=build --all",
    "dev": "nx run-many --target=serve --all --parallel",
    "test": "nx run-many --target=test --all",
    "lint": "nx run-many --target=lint --all",
    "typecheck": "nx run-many --target=typecheck --all",
    "affected:build": "nx affected --target=build",
    "affected:test": "nx affected --target=test",
    "affected:lint": "nx affected --target=lint",
    "serve:yes-no": "nx serve yes-no",
    "serve:1": "nx serve yes-no",
    "serve:cards": "nx serve cards",
    "serve:2": "nx serve cards",
    "serve:timer": "nx serve timer",
    "serve:3": "nx serve timer",
    "serve:repeat": "nx serve repeat",
    "serve:speak": "nx serve speak",
    "serve:match": "nx serve match",
    "serve:story": "nx serve story",
    "serve:sounds": "nx serve sounds",
    "serve:sequence": "nx serve sequence",
    "serve:board": "nx serve board",
    "serve:type": "nx serve type",
    "serve:4": "nx serve type",
    "serve:radio": "nx serve radio",
    "serve:5": "nx serve radio",
    "serve:todo": "nx serve todo",
    "serve:6": "nx serve todo",
    "serve:tiko": "nx serve tiko",
    "serve:7": "nx serve tiko",
    "serve:ui": "nx serve ui-docs",
    "serve:8": "nx serve ui-docs",
    "ios:build:all": "nx run-many --target=build --all && nx run-many --target=cap:sync --all",
    "ios:build:cards": "cd apps/cards && pnpm run ios:build",
    "ios:build:timer": "cd apps/timer && pnpm run ios:build",
    "ios:build:radio": "cd apps/radio && pnpm run ios:build",
    "ios:build:type": "cd apps/type && pnpm run ios:build",
    "ios:build:yes-no": "cd apps/yes-no && pnpm run ios:build",
    "ios:build:todo": "cd apps/todo && pnpm run ios:build",
    "ios:open:cards": "cd apps/cards && pnpm run cap:open",
    "ios:open:timer": "cd apps/timer && pnpm run cap:open",
    "ios:open:radio": "cd apps/radio && pnpm run cap:open",
    "ios:open:type": "cd apps/type && pnpm run cap:open",
    "ios:open:yes-no": "cd apps/yes-no && pnpm run cap:open",
    "ios:open:todo": "cd apps/todo && pnpm run cap:open"
  },
  "devDependencies": {
    "@capacitor/cli": "^7.4.1",
    "@capacitor/core": "^7.4.1",
    "@capacitor/ios": "^7.4.1",
    "@nx/cypress": "18.0.0",
    "@nx/eslint": "18.0.0",
    "@nx/js": "18.0.0",
    "@nx/vite": "18.0.0",
    "@nx/vue": "18.0.0",
    "@nx/web": "18.0.0",
    "@nx/workspace": "18.0.0",
    "@swc-node/register": "~1.6.7",
    "@swc/core": "~1.3.85",
    "@swc/helpers": "~0.5.2",
    "@types/node": "18.16.9",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-vue": "^4.6.2",
    "@vitest/coverage-v8": "^1.0.4",
    "@vitest/ui": "^1.0.4",
    "@vue/eslint-config-prettier": "7.1.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.1",
    "cypress": "^13.0.0",
    "eslint": "~8.57.0",
    "eslint-plugin-cypress": "^2.13.4",
    "eslint-plugin-vue": "^9.20.1",
    "jsdom": "~22.1.0",
    "nx": "18.0.0",
    "prettier": "^3.2.5",
    "sass": "^1.80.0",
    "typescript": "~5.3.3",
    "vite": "^5.1.4",
    "vite-plugin-pwa": "^1.0.1",
    "vitest": "^1.3.1",
    "vue-tsc": "^1.8.27"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.50.3",
    "bemm": "^1.2.2",
    "mitt": "^3.0.1",
    "pinia": "^2.1.7",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0"
  },
  "nx": {
    "includedScripts": []
  }
}
