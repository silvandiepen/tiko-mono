{
  "name": "tiko-mono",
  "version": "1.0.0",
  "description": "Tiko - Accessibility-focused communication apps for children",
  "private": true,
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "packageManager": "pnpm@8.15.0",
  "scripts": {
    "build": "nx run-many --target=build --all",
    "dev": "nx run-many --target=serve --all --parallel",
    "test": "nx run-many --target=test --all",
    "lint": "nx run-many --target=lint --all",
    "typecheck": "nx run-many --target=typecheck --all",
    "affected:build": "nx affected --target=build",
    "affected:test": "nx affected --target=test",
    "affected:lint": "nx affected --target=lint",
    "serve:yes-no": "nx serve yes-no",
    "serve:cards": "nx serve cards",
    "serve:timer": "nx serve timer",
    "serve:repeat": "nx serve repeat",
    "serve:speak": "nx serve speak",
    "serve:match": "nx serve match",
    "serve:story": "nx serve story",
    "serve:sounds": "nx serve sounds",
    "serve:sequence": "nx serve sequence",
    "serve:board": "nx serve board",
    "serve:type": "nx serve type"
  },
  "devDependencies": {
    "@nx/eslint": "18.0.0",
    "@nx/js": "18.0.0",
    "@nx/vite": "18.0.0",
    "@nx/vue": "18.0.0",
    "@nx/workspace": "18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-vue": "^4.6.2",
    "@vue/eslint-config-typescript": "^12.0.0",
    "eslint": "~8.57.0",
    "eslint-plugin-vue": "^9.20.1",
    "nx": "18.0.0",
    "prettier": "^3.2.5",
    "typescript": "~5.3.3",
    "vite": "^5.1.4",
    "vitest": "^1.3.1",
    "vue-tsc": "^1.8.27"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.50.3",
    "bemm": "^1.2.2",
    "pinia": "^2.1.7",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0"
  },
  "nx": {
    "includedScripts": []
  }
}